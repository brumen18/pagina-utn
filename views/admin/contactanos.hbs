<table>
    <thead>
        <th>Nombre</th>
        <th>Correo</th>
        <th>Mensaje</th>
    </thead>
    <tbody>
        <tr>
            {{#each datosContacto}}
            <td>{{nombre}} {{apellidos}}</td>
            <td>{{correo}}</td>
            <td>{{mensaje}}</td>
            <td>
                <a href="/admin/contactanos/eliminar/{{id}}">Eliminar consulta</a>
                <a href="/admin/contactanos/editarcontacto/{{id}}">Modificar consulta</a>
                </td>
        </tr>
        {{/each}}
    </tbody>
</table>